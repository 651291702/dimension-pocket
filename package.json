{
  "name": "dimension-pocket",
  "version": "0.0.1",
  "main": "build/_.js",
  "author": {
    "name": "gajon"
  },
  "scripts": {
    "dev": "concurrently -n=vue,ele -c=green,blue \"yarn dev:vite\" \"yarn dev:ele\"",
    "dev:vite": "vite",
    "dev:ele": "node script/build --env=development --watch",
    "build:vite": "vite build",
    "build:ele": "node script/build --env=production",
    "build": "yarn build:vite && yarn build:ele",
    "lint": "eslint --fix",
    "postinstall": "husky install",
    "pack-win": "electron-builder --win"
  },
  "dependencies": {
    "crypto-js": "^4.2.0",
    "electron-is-dev": "^2.0.0",
    "element-plus": "^1.0.1-beta.14",
    "eventemitter3": "^4.0.7",
    "ffmpeg-static": "^4.3.0",
    "fluent-ffmpeg": "^2.1.2",
    "got": "^11.8.2",
    "m3u8-parser": "^4.6.0",
    "nedb-promises": "^4.1.2",
    "tunnel": "^0.0.6",
    "url-toolkit": "^2.2.1",
    "vue": "^3.0.4",
    "vue-router": "4",
    "vuex": "^4.0.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^18.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^11.2.1",
    "@rollup/plugin-typescript": "^8.1.0",
    "@tailwindcss/postcss7-compat": "^2.0.1",
    "@types/crypto-js": "^4.2.2",
    "@types/fluent-ffmpeg": "^2.1.17",
    "@types/lowdb": "^1.0.9",
    "@types/nedb": "^1.8.11",
    "@types/node": "^14.14.41",
    "@types/tunnel": "^0.0.2",
    "@typescript-eslint/eslint-plugin": "^4.11.0",
    "@typescript-eslint/parser": "^4.11.0",
    "@vitejs/plugin-vue": "^1.2.1",
    "@vue/compiler-sfc": "^3.0.4",
    "autoprefixer": "9",
    "chalk": "^4.1.0",
    "concurrently": "^5.3.0",
    "dotenv": "^8.2.0",
    "electron": "^31.2.0",
    "electron-builder": "23",
    "electron-connect": "^0.6.3",
    "eslint": "^7.16.0",
    "husky": "^5.2.0",
    "lint-staged": "^10.5.4",
    "log4js": "^6.3.0",
    "minimist": "^1.2.5",
    "ora": "^5.1.0",
    "postcss": "7",
    "rollup": "^2.35.1",
    "rollup-plugin-node-builtins": "^2.1.2",
    "sass": "^1.30.0",
    "stylelint": "^13.12.0",
    "stylelint-config-recommended": "^4.0.0",
    "stylelint-config-standard": "^21.0.0",
    "stylelint-scss": "^3.19.0",
    "tailwindcss": "npm:@tailwindcss/postcss7-compat",
    "typescript": "^4.1.3",
    "vite": "^2.1.5",
    "wait-on": "^5.3.0"
  },
  "lint-staged": {},
  "build": {
    "appId": "cn.gajon.dimension-pocket",
    "productName": "dimension-pocket",
    "artifactName": "${productName}-${os}-${arch}-${version}.${ext}",
    "electronVersion": "14.1.1",
    "copyright": "Copyright Â© year ${author}",
    "compression": "normal",
    "asarUnpack": [
      "node_modules/ffmpeg-static/bin/${os}/${arch}/ffmpeg",
      "node_modules/ffmpeg-static/index.js",
      "node_modules/ffmpeg-static/package.json"
    ],
    "files": [
      "**/*",
      "build/**/*",
      "!dist/*"
    ],
    "win": {
      "icon": "public/icon.png"
    }
  },
  "license": "MIT"
}
