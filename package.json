{
  "name": "dimension-pocket",
  "version": "0.0.0",
  "main": "dist/main/_.js",
  "scripts": {
    "dev": "concurrently -n=vue,ele -c=green,blue \"npm run dev:vite\" \"npm run dev:ele\"",
    "dev:vite": "vite",
    "dev:ele": "node script/build --env=development --watch",
    "build": "vite build",
    "lint": "eslint --cache --fix"
  },
  "dependencies": {
    "element-plus": "^1.0.1-beta.14",
    "eventemitter3": "^4.0.7",
    "vue": "^3.0.4",
    "vue-router": "4"
  },
  "devDependencies": {
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-typescript": "^8.1.0",
    "@tailwindcss/postcss7-compat": "^2.0.1",
    "@typescript-eslint/eslint-plugin": "^4.11.0",
    "@typescript-eslint/parser": "^4.11.0",
    "@vue/compiler-sfc": "^3.0.4",
    "autoprefixer": "9",
    "chalk": "^4.1.0",
    "concurrently": "^5.3.0",
    "dotenv": "^8.2.0",
    "electron": "^11.1.0",
    "electron-builder": "^22.9.1",
    "electron-connect": "^0.6.3",
    "electron-is-dev": "^1.2.0",
    "eslint": "^7.16.0",
    "husky": "^5.2.0",
    "lint-staged": "^10.5.4",
    "log4js": "^6.3.0",
    "minimist": "^1.2.5",
    "ora": "^5.1.0",
    "postcss": "7",
    "rollup": "^2.35.1",
    "rollup-plugin-node-builtins": "^2.1.2",
    "sass": "^1.30.0",
    "stylelint-config-recommended": "^4.0.0",
    "tailwindcss": "npm:@tailwindcss/postcss7-compat",
    "typescript": "^4.1.3",
    "vite": "^1.0.0-rc.13",
    "wait-on": "^5.2.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,ts,tsx}": [
      "npm run lint",
      "git add"
    ]
  }
}
